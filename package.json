{
	"name": "esm-boilerplate",
	"version": "0.0.1",
	"description": "",
	"main": "index.ts",
	"type": "module",
	"scripts": {
		"start": "node --loader ts-node/esm --no-warnings src/index.ts",
		"server": "node --loader ts-node/esm --no-warnings src/server/index.ts",
		"dev": "snowpack dev",
		"build": "tsc -p .",
		"test": "mocha",
		"test:watch": "mocha -r ts-node/register --watch --watch-files src/**/*.ts,test/**/*.ts --parallel",
		"test:coverage": "c8 npm test",
		"test:wtr": "web-test-runner \"test/**/*.test.ts\" --puppeteer --watch",
		"lint": "eslint . --ext .ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@snowpack/plugin-typescript": "^1.2.1",
		"@snowpack/web-test-runner-plugin": "^0.2.2",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.155",
		"@types/chai": "^4.3.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "^17.0.21",
		"@typescript-eslint/eslint-plugin": "^5.15.0",
		"@typescript-eslint/parser": "^5.15.0",
		"@web/dev-server-import-maps": "^0.0.6",
		"@web/test-runner": "^0.13.27",
		"@web/test-runner-puppeteer": "^0.10.5",
		"c8": "^7.11.0",
		"chai": "^4.3.6",
		"eslint": "^8.11.0",
		"mocha": "^9.2.2",
		"snowpack": "^3.8.8",
		"ts-node": "^10.7.0",
		"tslib": "^2.3.1",
		"typescript": "^4.6.2"
	},
	"c8": {
		"reporter": [
			"text",
			"html"
		]
	},
	"dependencies": {
		"koa": "^2.13.4"
	}
}
